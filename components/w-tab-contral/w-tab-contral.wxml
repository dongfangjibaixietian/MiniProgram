<!--components/w-tab-contral/w-tab-contral.wxml-->
<view class="tab-contral">
      <block wx:for="{{titles}}" wx:key="{{index}}">
          <view class='tab-item {{currentIndex == index ? "active": ""}}'
            bindtap="handleItemClick"
            data-index="{{index}}">
            {{item}}
          </view>
      </block>
</view>